{
  "name": "prodmon",
  "version": "1.0.0",
  "description": "Productivity Monkey - Team productivity monitoring and analytics",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"tsc -w\" \"electron .\"",
    "prebuild": "node scripts/generate-icons.js",
    "build": "npm run build:native:optional && npm run prebuild && tsc && electron-builder",
    "build:tsc": "tsc",
    "build:native": "node-gyp rebuild",
    "build:native:optional": "node-gyp rebuild || echo \"Native module build failed - Windows URL capture will not be available\"",
    "build:client": "npm run build:native:optional && npm run prebuild && tsc && electron-builder",
    "build:client:mac": "npm run prebuild && tsc && electron-builder --mac",
    "build:client:win": "npm run prebuild && tsc && electron-builder --win",
    "build:client:linux": "npm run prebuild && tsc && electron-builder --linux",
    "build:server": "tsc && mkdir -p dist/server/public && cp -r src/server/public/* dist/server/public/",
    "agent": "node dist/agent/index.js",
    "dashboard": "electron .",
    "test": "jest",
    "server": "node dist/server/server.js",
    "server:dev": "concurrently \"tsc -w\" \"nodemon dist/server/server.js\"",
    "server:pm2:start": "pm2 start dist/server/server.js --name prodmon-server",
    "server:pm2:stop": "pm2 stop prodmon-server",
    "server:pm2:restart": "pm2 restart prodmon-server",
    "server:pm2:delete": "pm2 delete prodmon-server",
    "server:pm2:logs": "pm2 logs prodmon-server",
    "server:pm2:status": "pm2 status prodmon-server"
  },
  "keywords": [
    "productivity",
    "monitoring",
    "analytics"
  ],
  "author": {
    "name": "Productivity Monkey Team",
    "email": "prodmon@example.com"
  },
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.5",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "get-windows": "^9.2.3",
    "google-auth-library": "^10.4.0",
    "jsonwebtoken": "^9.0.2",
    "node-addon-api": "^8.0.0",
    "node-gyp": "^10.0.1",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "electron": "^38.2.2",
    "electron-builder": "^24.9.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sharp": "^0.34.4",
    "typescript": "^5.3.3"
  }
}
